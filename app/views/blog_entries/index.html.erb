<div class="row centers">
  <div class="span12 centers">
    <legend class="blue-wrap" id="align"><strong>Comments</strong></legend>
    <span>
        <ul class="nav nav-pills offset1">
          <% @blog_all.each do |f| %>
            <li class="span3" id="align">
              <a href= <%= blog_entry_path f.id %> ><h4><%= f.title %></h4></a>
            </li>
          <% end %>
        </ul>
    </span>
  </div>
</div>